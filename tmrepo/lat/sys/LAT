mount -o rw,remount / 2>/dev/null
mount -o rw,remount /system 2>/dev/null
mount -o rw,remount /data 2>/dev/null

CYN='\033[0;36m'
GRN='\033[0;32m'
RED='\033[0;31m'
NC='\033[0m'

mainPATH=/data/lat
exPATH=$mainPATH/sys
clear
echo -e "${GRN}"
cat $exPATH/b
sleep 2
clear
echo -e "${RED}"
cat $exPATH/a
sleep 2
echo -e "${GRN}"
$exPATH/return


